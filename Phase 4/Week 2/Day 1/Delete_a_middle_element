import java.util.Stack;

class Solution {
    public void deleteMid(Stack<Integer> s, int size) {
        delete(s, (size + 1) / 2);
    }

    private void delete(Stack<Integer> s, int k) {
        if (k == 1) {
            s.pop();
            return;
        }

        int top = s.pop();
        delete(s, k - 1);
        s.push(top);
    }
}
